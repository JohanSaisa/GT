@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

@model List<GT.Core.DTO.Impl.ListingInquiryDTO>
@{
	ViewData["Title"] = "Ansökningar";
	ViewData["JobTitle"] = "Placeholder att ersättas av controller";
}

<h5 class="modal-title" id="staticBackdropLabel">Applications for @ViewBag["JobTitle"]. </h5>
<div>
	@foreach (var item in Model)
	{
			<div class="card-body">
			@Html.ActionLink(item.MessageTitle, "GetInquiry", new { id=item.Id }, new { @class="h5 card-title stretched-link" })
				<p class="card-text">@item.LinkedInLink</p>
			@Html.ActionLink("Edit", "EditInquiry", new { id=item.Id }, new { @class="btn btn-alert"})
			@Html.ActionLink("Delete", "DeleteInquiry", new { id=item.Id }, new { @class="btn btn-warning"})
			</div>
	}
</div>
