@model GT.Core.DTO.Impl.CompanyLogoDTO

@{
    ViewData["Title"] = "CompanyLogoUpload";

}
<form asp-action="Upload" asp-controller="Upload" method="post" enctype = "multipart/form-data">
    
    <div class="row mt-2">
        <div class="col-12">
            <label class="col-form-label">Enter companyId</label>
            <input asp-for="CompanyId" class="form-control" />
            <span asp-validation-for="CompanyId" class="text-danger"></span>
        </div>
    </div>

    <div class="row mt-2">
        <div class="col-12">
            <label class="col-form-label">Select File</label>
            <input asp-for="File" class="form-control" />
            <span asp-validation-for="File" class="text-danger"></span>
        </div>
    </div>


    <div class="row mt-2">
        <div class="col-12">
            <button type="submit" class="btn btn-success">Upload File</button>
        </div>
    </div>
</form>
